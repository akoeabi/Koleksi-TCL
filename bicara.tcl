bind pubm - "story_mbemquitstory_mbem" quit_speak 
bind pubm - "story_mbemkuitstory_mbem" quit_speak 
bind pubm - "story_mbem aku muleh story_mbem" quit_speak 
bind pubm - "story_mbem meh muleh story_mbem" quit_speak 
bind pubm - "lerenstory_mbem" quit_speak 
bind pubm - "mulehstory_mbem" quit_speak 
bind pubm - "balekstory_mbem" quit_speak
bind pubm - "pulangstory_mbem" quit_speak
bind pubm - "pamitstory_mbem" quit_speak
set ranquit {
  "Bentar Lagi Donk"
  "Dah..!!!"
  "Hati-Hati YaH"
  "TT DJ"
  "ko ga pamit seh"
 }

proc quit_speak {nick uhost hand chan text} {
 global botnick quit_chans ranquit
if {(([lsearch -exact [string tolower $quit_chans] [string tolower $chan]] != -1) || ($quit_chans == "story_mbem"))} {
set quits [lindex $ranquit [rand [llength $ranquit]]]
putserv "PRIVMSG $chan :$quits $nick"
  }
}

